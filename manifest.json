{
	"name": "Cookie API Test Extension",
	"version": "0.8",
	"description": "Testing Cookie API",
	"permissions": ["cookies", "tabs", "http://*/*", "https://*/*"],
	"icons": {
		"16": "img/cookie.png",
		"48": "img/cookie.png",
		"128": "img/cookie.png"
	},
	"browser_action": {
		"default_icon": "img/cookie.png"
	},
	"background": {
		"scripts": ["scripts/background.js"]
	},
	"manifest_version": 2
}
